    "homepage": "http://clickdiscover.io",
    "license": "proprietary",
    "description": "Landing Page Templating and Event Tracking",
    "require": {
        "php": ">=5.5",
        "slim/slim": ">=3.0",
        "league/plates": "^3.1",
        "league/flysystem": "^1.0",
        "tedivm/stash": "^0.12.3",
        "monolog/monolog": "^1.15",
        "f3ath/lazypdo": "^0.0.1",
        "pimple/pimple": "^3.0",
        "acclimate/container": "^1.0.0",
        "punic/punic": "^1.6",
        "php-units-of-measure/php-units-of-measure": "^2.0",
        "slim/php-view": "^2.0",
        "oscarotero/psr7-middlewares": "^3.12",
        "slim/twig-view": "^2.0",
        "components/jquery": "^2.2",
        "robloach/component-installer": "^0.2.3",
        "league/fractal": "^0.13.0",
        "fzaninotto/faker": "^1.5"
